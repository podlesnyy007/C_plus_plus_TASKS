#include <iostream>
using namespace std;

void f(int &x) {
	if (x % 2 == 0) {
		x /= 2;
	}
	else {
		x = 0;
	}
}

int main() {
	int x;
	cin >> x;
	f(x);
	cout << x;
}



#include <iostream>
using namespace std;
#include<algorithm>

int main() {
	int n, k = 0, x;
	cin >> n;
	while (n < k) {
		int s;
		cin >> s;
		x = max(x, s);
		k++;
	}
	cout << x;
}
